  .navigator {
    background:#f8f9fc;
    grid-column-start: 1;
    grid-column-end: 2;
    grid-row-start: 1;
    grid-row-end: 4;
    grid-auto-flow: column;
      max-width: 100%;
      height:100%
  }
  .menu{
      padding:2em;
          list-style-type: none;
  }
  .menu li{
    border: .5px solid #66666638;
    padding: 1em;
    text-align: center;
        margin-bottom: 1em;
  }
  .menu li a{
      color: #c00000;
      font-family:"Raleway";
          text-decoration: none;
    font-weight: bold;
  }